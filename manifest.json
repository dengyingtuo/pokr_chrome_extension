{
  "name": "POKR",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Personal Objectives and Key Results",
  "homepage_url": "http://tapley.co",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "management",
    "storage"
  ],
  "chrome_url_overrides" : {
    "newtab": "/src/index.html"
  },
  "content_scripts" : [ {
    "matches": ["http://*/*"],
     "js": [ "/js/jquery/jquery.min.js"]
   }]
}
